#include <stdio.h>

int main()
{
    int Array[11], i, n ,sum=0;
    
    printf("Enter the size of the Array: ");
    scanf("%d", &n);
    
    for(i = 1; i <= n; ++i)
    {
        printf("Enter Element %d: ", i);
        scanf("%d", &Array[i]);
        
        sum += Array[i];
    }
    printf("Sum of All Array Elements = %d", sum);
    return 0;
}
